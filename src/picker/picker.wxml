<view class="{{classPrefix}}">
  <view class="{{classPrefix}}__toolbar">
    <view class="{{classPrefix}}__cancel" bindtap="onCancel"> {{cancelButtonText}} </view>
    <view class="{{classPrefix}}__title"> {{title}} </view>
    <view class="{{classPrefix}}__confirm" bindtap="onConfirm"> {{confirmButtonText}} </view>
  </view>
  <view class="{{classPrefix}}__main">
    <slot></slot>
    <view class="{{classPrefix}}__mask"></view>
    <view class="{{classPrefix}}__indicator"></view>
  </view>
</view>
