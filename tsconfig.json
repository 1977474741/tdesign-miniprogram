{
  "compilerOptions": {
    "module": "commonjs",
    "experimentalDecorators": true,
    "target": "es5",
    "lib": ["es2015", "es2017", "dom"],
    "outDir": "dist",
    "types": ["miniprogram-api-typings"],
    "skipLibCheck": true,
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@tencent/tdesign-miniprogram/*": ["miniprogram_dist/*"]
    }
  },
  "include": ["src/**/*", "example/**/*", "site/**/*", "typing.d.ts"]
}
